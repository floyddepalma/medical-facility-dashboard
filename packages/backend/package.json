{
  "name": "@medical-facility-dashboard/backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend API server for medical facility dashboard",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --testPathIgnorePatterns=\\.property\\.test\\.ts$",
    "test:property": "jest --testMatch=\"**/*.property.test.ts\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "db:migrate": "node -r tsx/register src/db/migrate.ts",
    "db:seed": "node -r tsx/register src/db/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "zod": "^3.22.4",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/pg": "^8.10.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "fast-check": "^3.15.0",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "typescript-eslint": "^8.0.0"
  }
}
